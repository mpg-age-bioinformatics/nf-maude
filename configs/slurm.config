process {
    withName:prodrugz {
        executor='slurm'
        queue="${params.queue}"
        cpus=1
        memory='20GB'
        time='1h'
        container = "${params.image_folder}maude-7aa20cc.sif"
        containerOptions = "${params.extra_mounts} -B ${params.output_drugz}:${params.output_maude} -B ${params.ouput_mageck_count}:${params.ouput_mageck_count} -B ${params.facs}:${params.facs} -B ${params.ctrl_guides}:${params.ctrl_guides}"
    }
}

